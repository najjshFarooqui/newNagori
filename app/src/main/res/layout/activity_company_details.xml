<?xml version="1.0" encoding="utf-8"?>
<layout>    <LinearLayout tools:context=".MainActivity"
    android:background="#E0FFFF"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <TableLayout
        android:background="#1C1818"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="30dp"
        android:id="@+id/table">


        <TableRow>
            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginHorizontal="1dp"
                android:gravity="center"
                android:layout_weight="1"
                android:text="SNO"
                android:layout_marginBottom="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="OEM"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="Tel Part Number"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginHorizontal="1dp"
                android:gravity="center"
                android:layout_weight="1"
                android:text="Engine"
                android:layout_marginBottom="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="Application"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="Mrp"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="Orientation-alpha"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="Orientation-Beta"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="str-pre"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="setting-pre"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="Lift"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>

            <TextView android:background="#90EE90"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:text="Reman"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"/>
        </TableRow>
    </TableLayout>
</LinearLayout>
</layout>


